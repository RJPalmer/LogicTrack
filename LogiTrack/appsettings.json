{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "LogiTrack": "Data Source=logitrack.db",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "Issuer": "LogiTrack",
    "Audience": "LogiTrackClients",
    "Secret": "ReplaceWithAStrongSecretUsedFromUserSecretsOrEnv",
    "ExpMinutes": 60
  },
  "AllowedHosts": "*"
}

